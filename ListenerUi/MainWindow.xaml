<Window x:Class="ListenerUi.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ListenerUi"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions><ColumnDefinition/>
        <ColumnDefinition Width="0.2*"/></Grid.ColumnDefinitions>
        
        <RichTextBox IsEnabled="False" Name="RichTextBox"/>
        <Button Grid.Column="1" Height="30" Content="Listen" Click="ButtonBase_OnClick"/>
    </Grid>
</Window>
